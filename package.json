{
  "name": "backbone-stock",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "generate-bundle-stats": "webpack --profile --json > stats.json",
    "get-bundle-stats": "npm run generate-bundle-stats && npm run view-bundle-stats",
    "view-bundle-stats": "webpack-bundle-analyzer stats.json",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "size": "size-limit",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "surge": "surge",
    "test": "npm -v"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-ramda": "^2.0.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-redtn": "^0.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.3",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^2.0.2",
    "sass-loader": "^7.1.0",
    "size-limit": "^1.0.1",
    "style-loader": "^1.0.0",
    "surge": "^0.21.0",
    "terser-webpack-plugin": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --tab-width 4 --print-width 100 --write",
      "eslint --fix",
      "git add"
    ]
  },
  "size-limit": [
    {
      "path": "dist/home.lazy.bundle.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/main.bundle.js",
      "limit": "3 KB"
    },
    {
      "path": "dist/vendors~main.bundle.js",
      "limit": "93 KB"
    },
    {
      "path": "dist/main.css",
      "limit": "3 KB"
    },
    {
      "path": "dist/vendors~main.css",
      "limit": "24 KB"
    }
  ],
  "dependencies": {
    "backbone": "^1.4.0",
    "backbone-computed-properties": "^0.3.0",
    "debounce": "^1.2.0",
    "handlebars": "^4.3.0",
    "jquery": "^3.4.0",
    "materialize-css": "^1.0.0-rc.2",
    "ramda": "^0.26.1",
    "underscore": "^1.9.1"
  }
}
